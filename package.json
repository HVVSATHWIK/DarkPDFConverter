{
  "name": "litasdark-react",
  "private": true,
  "description": "Privacy-first PDF editor powered by Rust & WebAssembly. Process files locally with a stunning Liquid Glass UI.",
  "keywords": [
    "pdf",
    "rust",
    "wasm",
    "webassembly",
    "react",
    "privacy",
    "dark-mode"
  ],
  "homepage": "https://litasdark-git-main-veerendranath-venkata-sathwiks-projects.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/HVVSATHWIK/DarkPDFConverter.git"
  },
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "typecheck": "tsc -b",
    "lint": "eslint src vite.config.* vitest.config.* tailwind.config.* postcss.config.js",
    "lint:fix": "eslint src vite.config.* vitest.config.* tailwind.config.* postcss.config.js --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build:wasm": "cd src-wasm && wasm-pack build --target web"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@react-spring/three": "^9.7.3",
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.16",
    "@types/lodash": "^4.17.16",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.17",
    "firebase": "^12.3.0",
    "framer-motion": "^11.0.5",
    "gl-react": "^5.2.0",
    "gl-react-dom": "^5.2.1",
    "glsl-noise": "^0.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.562.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.296",
    "postcss": "^8.4.35",
    "postprocessing": "^6.38.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-pdf": "^10.3.0",
    "react-router-dom": "^6.23.0",
    "react-window": "^1.8.10",
    "tailwindcss": "^3.4.1",
    "three": "^0.161.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "jsdom": "^26.1.0",
    "lru-cache": "^11.2.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^5.1.3",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4",
    "wasm-pack": "^0.13.0"
  },
  "overrides": {
    "three-mesh-bvh": "0.8.0"
  }
}